# clangd 配置文件，由 clangd_config_generator 生成
CompileFlags:
  Add:
    - -I.
    - -Iinclude
    - -Isrc
    - -Isrc/app
    - -Isrc/brom
    - -Isrc/demo
    - -Isrc/drivers
    - -Isrc/test
    - -D_GNU_SOURCE
    - -DCURRENT_MODULE_STATIC_EN=$(STATIC_EN_VAL)
    - -DWW_LOG_ENCODE_RAM_BUFFER_EN
    - -DCURRENT_MODULE_STATIC_EN=0
    - -D__NOTDIR_FILE__=\"$(notdir
    - -DWW_LOG_STATIC_MODULE_DEMO_EN=0\""
    - -DWW_LOG_STATIC_MODULE_TEST_EN=0\""
    - -DWW_LOG_STATIC_MODULE_DEMO_EN=0
    - -DCURRENT_MODULE_ID=$(MODULE_ID_VAL)
    - -DCURRENT_FILE_ID=$(FILE_ID_VAL)
    - -DWW_LOG_STATIC_MODULE_DEMO_EN=0"
    - -DDEBUG=1
    - -DWW_LOG_STATIC_MODULE_TEST_EN=0"
  Remove:
    - -W*
    - -std=*
    - -O*

Diagnostics:
  UnusedIncludes: false
  MissingIncludes: false

InlayHints:
  Enabled: true
  ParameterNames: true
  VariableTypes: true
  DeducedTypes: true

Index:
  Background: Build
  StandardLibrary: true
